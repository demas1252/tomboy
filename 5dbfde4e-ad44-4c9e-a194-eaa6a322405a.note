&lt;&lt;pre
(define x 3)

(define (<link:internal>inc</link:internal>-x)
  (begin 
    (set! x (+ x <link:internal>1</link:internal>))
    x))
    
    
&gt; (<link:internal>inc</link:internal>-x)
